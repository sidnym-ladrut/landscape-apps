{
  "name": "landscape-apps",
  "version": "3.3.0",
  "private": true,
  "engines": {
    "node": "16.19.1",
    "npm": "8.19.3"
  },
  "scripts": {
    "prepare": "cd .. && husky install ui/.husky",
    "build": "tsc && vite build",
    "build:mock": "tsc && vite build --mode staging",
    "build:profile": "tsc && vite build --mode profile",
    "build:chatmock": "tsc && cross-env APP=chat vite build --mode staging",
    "build:chat": "tsc && cross-env APP=chat vite build",
    "coverage": "cross-env TZ=UTC vitest run --coverage",
    "dev": "cross-env SSL=true vite",
    "dev:native": "cross-env APP=chat vite --mode native --host",
    "dev-no-ssl": "vite",
    "dev2": "cross-env SSL=true vite --mode dev2",
    "multi": "cross-env SSL=true concurrently \"vite\" \"vite --mode dev2\"",
    "dev-sw": "vite --mode sw",
    "chat-sw": "cross-env APP=chat vite --mode sw",
    "chat": "cross-env APP=chat SSL=true vite --host",
    "chat-no-ssl": "cross-env APP=chat vite --host",
    "multichat": "cross-env APP=chat SSL=true concurrently \"vite\" \"vite --mode dev2\"",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix --quiet",
    "lint:format": "prettier src/ --write",
    "lint:all": "npm run lint:format && npm run lint:fix",
    "mock": "vite --mode mock",
    "mock:chat": "cross-env APP=chat vite --mode mock",
    "serve": "vite preview",
    "serve-sw": "vite preview --mode sw",
    "serve:chat": "cross-env APP=chat vite preview",
    "serve:chat-sw": "cross-env APP=chat vite preview --mode sw",
    "test": "cross-env TZ=UTC vitest",
    "e2e": "../rube/pre_playwright.sh && cross-env SHIP=~bus npx playwright test --workers=1 && cross-env SHIP=~zod npx playwright test --workers=1 && ../rube/post_playwright.sh",
    "e2e:debug:zod": "../rube/pre_playwright.sh && cross-env SHIP=~zod npx playwright test --debug && ../rube/post_playwright.sh",
    "e2e:debug:bus": "../rube/pre_playwright.sh && cross-env SHIP=~bus npx playwright test --debug && ../rube/post_playwright.sh",
    "e2e:debug": "../rube/pre_playwright.sh && cross-env SHIP=~bus npx playwright test --debug && cross-env SHIP=~zod npx playwright test --debug && ../rube/post_playwright.sh",
    "e2e:chat": "../rube/pre_playwright.sh && cross-env SHIP=~zod APP=chat npx playwright test && cross-env SHIP=~bus APP=chat npx playwright test && ../rube/post_playwright.sh",
    "tsc": "tsc --noEmit",
    "postinstall": "patch-package",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "e2e:codegen": "concurrently \"npm run dev-no-ssl\" \"npx playwright codegen http://localhost:3000\""
  },
  "lint-staged": {
    "**/*.ts?(x)": [
      "tsc-files --noEmit src/window.ts src/env.d.ts src/global.d.ts"
    ],
    "*.{js,jsx,ts,tsx}": [
      "npm run lint:fix",
      "npm run lint:format"
    ]
  },
